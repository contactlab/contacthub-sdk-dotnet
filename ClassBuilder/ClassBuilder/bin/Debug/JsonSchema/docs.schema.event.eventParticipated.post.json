{
  "$schema": "http://api.contactlab.it/hub#",
  "type": "object",
  "description": "Customer has participated to an event",
  "properties": {
    "customerId": {
      "type": "string",
      "pattern": "^.+$"
    },
    "bringBackProperties": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "ref": "https://api.contactlab.it/hub/v1/docs/schema/enums.json#/definitions/EventBringBackProperties"
        }
      ]
    },
    "type": {
      "type": "string",
      "enum": [
        "eventParticipated"
      ]
    },
    "context": {
      "ref": "https://api.contactlab.it/hub/v1/docs/schema/enums.json#/definitions/EventContext"
    },
    "date": {
      "type": "string",
      "format": "date-time"
    },
    "contextInfo": {
      "type": "object"
    },
    "tracking": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "ref": "https://api.contactlab.it/hub/v1/docs/schema/event/tracking.json"
        }
      ]
    },
    "properties": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "eventId": {
          "type": "string",
          "description": "The idenfier of the event"
        },
        "eventName": {
          "type": "string",
          "description": "The name of the event"
        },
        "eventDate": {
          "type": "string",
          "description": "The date-time of the event",
          "format": "date-time"
        },
        "eventType": {
          "type": "string",
          "description": "The type of the event (e.g.: Cocktail, Meeting, etc)"
        },
        "eventLocation": {
          "type": [
            "object",
            "null"
          ],
          "description": "The location of the event",
          "contactlabProperties": {
            "label": "Event Location",
            "enabled": true
          },
          "properties": {
            "type": {
              "type": [
                "string",
                "null"
              ],
              "enum": [
                "STORE",
                "VENUE",
                "STADIUM",
                "MUSEUM",
                "HEADQUARTERS",
                "PALACE",
                "CLUB",
                "ROOM",
                "OTHER"
              ],
              "description": "The type of location",
              "contactlabProperties": {
                "label": "The location type",
                "enabled": true
              }
            },
            "id": {
              "type": "string",
              "description": "The idenfier of the location",
              "contactlabProperties": {
                "label": "The location identifier",
                "enabled": true
              }
            },
            "name": {
              "type": "string",
              "description": "The name of the location",
              "contactlabProperties": {
                "label": "The location name",
                "enabled": true
              }
            },
            "address": {
              "type": [
                "object",
                "null"
              ],
              "description": "The address details of the location",
              "contactlabProperties": {
                "label": "Address",
                "enabled": true
              },
              "additionalProperties": false,
              "properties": {
                "street": {
                  "type": [
                    "string",
                    "null"
                  ],
                  "description": "The location address street component",
                  "contactlabProperties": {
                    "label": "Street",
                    "enabled": true
                  }
                },
                "city": {
                  "type": [
                    "string",
                    "null"
                  ],
                  "description": "The location address city component",
                  "contactlabProperties": {
                    "label": "City",
                    "enabled": true
                  }
                },
                "country": {
                  "type": [
                    "string",
                    "null"
                  ],
                  "description": "The location address country component",
                  "contactlabProperties": {
                    "label": "Country",
                    "enabled": true
                  }
                },
                "province": {
                  "type": [
                    "string",
                    "null"
                  ],
                  "description": "The location address province component",
                  "contactlabProperties": {
                    "label": "Province",
                    "enabled": true
                  }
                },
                "region": {
                  "type": [
                    "string",
                    "null"
                  ],
                  "description": "The location address region component",
                  "contactlabProperties": {
                    "label": "Region",
                    "enabled": true
                  }
                },
                "zip": {
                  "type": [
                    "string",
                    "null"
                  ],
                  "description": "The location address ZIP code component",
                  "contactlabProperties": {
                    "label": "ZIP",
                    "enabled": true
                  }
                },
                "geo": {
                  "contactlabProperties": {
                    "label": "Geographic location",
                    "enabled": true
                  },
                  "oneOf": [
                    {
                      "type": "null"
                    },
                    {
                      "ref": "https://api.contactlab.it/hub/v1/docs/schema/enums.json#/definitions/GeographicLocation"
                    }
                  ],
                  "description": "The geographic coordinate of the location"
                }
              }
            }
          }
        },
        "checkIn": {
          "type": "string",
          "description": "The date-time of the check-in",
          "format": "date-time"
        },
        "checkOut": {
          "type": "string",
          "description": "The date-time of the check-out",
          "format": "date-time"
        },
        "host": {
          "type": "object",
          "description": "host of the event",
          "additionalProperties": false,
          "properties": {
            "firstName": {
              "type": "string",
              "description": "first name of the host"
            },
            "lastName": {
              "type": "string",
              "description": "last name of the host"
            },
            "title": {
              "type": "string",
              "description": "title of the hosting event"
            },
            "contacts": {
              "type": "object",
              "description": "contacts of the host",
              "additionalProperties": false,
              "properties": {
                "email": {
                  "type": "string",
                  "description": "email of the host",
                  "format": "email"
                },
                "phone": {
                  "type": "string",
                  "description": "phone number of the host"
                },
                "mobilePhone": {
                  "type": "string",
                  "description": "mobile phone number of the host"
                }
              }
            }
          }
        },
        "extraProperties": {
          "type": "object",
          "description": "Extra pieces of information you can tie to events you track"
        },
        "extended": {
          "oneOf": [
            {
              "type": "null"
            },
            {
              "ref": "https://api.contactlab.it/hub/v1/docs/schema/empty-extended-properties.json"
            }
          ]
        }
      }
    }
  },
  "required": [
    "type",
    "context",
    "properties"
  ],
  "additionalProperties": false
}