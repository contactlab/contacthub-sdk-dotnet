{
  "$schema": "http://api.contactlab.it/hub#",
  "type": "object",
  "description": "Customer viewed a product category list",
  "properties": {
    "customerId": {
      "type": "string",
      "pattern": "^.+$"
    },
    "bringBackProperties": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "ref": "https://api.contactlab.it/hub/v1/docs/schema/enums.json#/definitions/EventBringBackProperties"
        }
      ]
    },
    "type": {
      "type": "string",
      "enum": [
        "viewedProductCategory"
      ]
    },
    "context": {
      "ref": "https://api.contactlab.it/hub/v1/docs/schema/enums.json#/definitions/EventContext"
    },
    "date": {
      "type": "string",
      "format": "date-time"
    },
    "contextInfo": {
      "type": "object"
    },
    "tracking": {
      "ref": "https://api.contactlab.it/hub/v1/docs/schema/event/tracking.json#"
    },
    "properties": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "category": {
          "type": "string",
          "description": "Product category being viewed"
        },
        "extraProperties": {
          "type": "object",
          "description": "Extra pieces of information you can tie to events you track"
        }
      }
    }
  },
  "required": [
    "type",
    "context"
  ],
  "additionalProperties": false
}