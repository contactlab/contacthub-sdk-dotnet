{
  "type": "object",
  "$schema": "http://api.contactlab.it/hub#",
  "additionalProperties": false,
  "properties": {
    "client": {
      "type": "object",
      "properties": {
        "userAgent": {
          "type": "string"
        },
        "ip": {
          "type": "string",
          "format": "ipv4"
        }
      }
    },
    "store": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "type": {
          "type": "string",
          "enum": [
            "AIRPORT",
            "ECOMMERCE",
            "FLAGSHIP",
            "FREE-STANDING",
            "MALL",
            "OUTLET",
            "RESORT",
            "SIS",
            "WAREHOUSE",
            "NOT-DEFINED"
          ]
        },
        "street": {
          "type": "string"
        },
        "city": {
          "type": "string"
        },
        "country": {
          "type": "string"
        },
        "province": {
          "type": "string"
        },
        "zip": {
          "type": "string"
        },
        "geo": {
          "type": "object",
          "properties": {
            "lat": {
              "type": "number",
              "description": "latitude"
            },
            "lon": {
              "type": "number",
              "description": "longitude"
            }
          }
        },
        "website": {
          "type": "string"
        }
      }
    }
  }
}
